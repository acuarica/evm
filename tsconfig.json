{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node16",
        "target": "es2015",
        "checkJs": true,
        "strict": true,
        "noEmit": true,
        "exactOptionalPropertyTypes": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noPropertyAccessFromIndexSignature": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}

