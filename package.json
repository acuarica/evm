{
    "name": "sevm",
    "version": "0.2.0",
    "description": "An ethereum virtual machine (EVM) bytecode decompiler",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "default": "./dist/src/index.js"
        },
        "./opcode": {
            "types": "./dist/src/opcode.d.ts",
            "default": "./dist/src/opcode.js"
        },
        "./evm": {
            "types": "./dist/src/evm/index.d.ts",
            "default": "./dist/src/evm/index.js"
        }
    },
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts"
    ],
    "repository": "acuarica/evm",
    "author": "Luis Mastrangelo (https://acuarica.github.io)",
    "contributors": [
        "Luit Hollander <luit@hollander.email> (https://luit.me)"
    ],
    "license": "MIT",
    "keywords": [
        "4byte",
        "blockchain",
        "bytecode",
        "cli",
        "control-flow-graph",
        "decoder",
        "decompiler",
        "disassembler",
        "ethereum",
        "evm",
        "parser",
        "signatures",
        "solidity",
        "typescript",
        "web3",
        "yul"
    ],
    "devDependencies": {
        "@noble/hashes": "^1.3.0",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.7.2",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "chai": "^4.3.6",
        "eslint": "^8.36.0",
        "eslint-plugin-chai-expect": "^3.0.0",
        "eslint-plugin-mocha": "^10.1.0",
        "ethers": "^6.4.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.1",
        "mocha": "^10.0.1",
        "nyc": "^15.1.0",
        "prettier": "^2.8.4",
        "pretty-quick": "^3.1.3",
        "solc": "^0.8.21",
        "typedoc": "^0.24.6",
        "typedoc-plugin-extras": "^2.3.3",
        "typescript": "^5.0.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "scripts": {
        "prepare": "husky install",
        "compile": "tsc",
        "lint": "eslint .",
        "test": "yarn compile && mocha",
        "coverage": "yarn compile && nyc mocha",
        "bundle": "webpack --mode production",
        "make:ercs": " node selectors/ercs.mjs > src/ercs.ts",
        "prettier": "prettier --write .",
        "prettier:staged": "pretty-quick --staged",
        "docs": "http-server docs",
        "tsdoc": "typedoc --out docs/tsdoc src/index.ts src/opcode.ts src/evm/index.ts",
        "clean": "rm -r .solc/"
    }
}
