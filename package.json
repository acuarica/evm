{
    "name": "@acuarica/evm",
    "version": "0.0.7",
    "description": "An ethereum virtual machine (EVM) bytecode decompiler",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "default": "./dist/src/index.js"
        },
        "./opcode": {
            "types": "./dist/src/opcode.d.ts",
            "default": "./dist/src/opcode.js"
        },
        "./evm": {
            "types": "./dist/src/evm/index.d.ts",
            "default": "./dist/src/evm/index.js"
        },
        "./selector": {
            "types": "./dist/src/selector/index.d.ts",
            "default": "./dist/src/selector/index.js"
        }
    },
    "files": [
        "dist/src/"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/acuarica/evm.git"
    },
    "author": "Luis Mastrangelo (https://acuarica.github.io)",
    "contributors": [
        "Luit Hollander <luit@hollander.email> (https://luit.me)"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/acuarica/evm/issues"
    },
    "homepage": "https://github.com/acuarica/evm#readme",
    "keywords": [
        "ethereum",
        "evm",
        "utils",
        "decompiler",
        "typescript",
        "bytecode",
        "solidity"
    ],
    "devDependencies": {
        "@noble/hashes": "^1.3.0",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.7.2",
        "@types/semver": "^7.3.12",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "ansi-colors": "^4.1.1",
        "chai": "^4.3.6",
        "eslint": "^8.36.0",
        "ethers": "^5.6.9",
        "http-server": "^14.1.1",
        "husky": "^8.0.1",
        "mocha": "^10.0.1",
        "nyc": "^15.1.0",
        "prettier": "^2.8.4",
        "pretty-quick": "^3.1.3",
        "semver": "^7.3.7",
        "solc-0.5.17": "npm:solc@0.5.17",
        "solc-0.5.5": "npm:solc@0.5.5",
        "solc-0.6.12": "npm:solc@0.6.12",
        "solc-0.7.6": "npm:solc@0.7.6",
        "solc-0.8.16": "npm:solc@0.8.16",
        "typescript": "^5.0.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "scripts": {
        "prepare": "husky install",
        "compile": "tsc",
        "lint": "eslint .",
        "test": "yarn compile && mocha 'dist/test/**/*.test.js'",
        "test:check": "yarn compile && mocha 'dist/test/*.check.js'",
        "test:smoke": "yarn compile && mocha 'dist/test/*.smoke.js'",
        "coverage": "yarn compile && nyc mocha 'dist/test/**/*.{test,check}.js'",
        "bundle": "webpack --mode production",
        "make:selectors": "node selectors/make.js",
        "prettier": "prettier --write .",
        "prettier:staged": "pretty-quick --staged",
        "docs": "http-server docs"
    }
}
