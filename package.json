{
    "name": "@acuarica/evm",
    "version": "0.0.4",
    "description": "An ethereum virtual machine (EVM) bytecode decompiler",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "default": "./dist/src/index.js"
        },
        "./opcode": {
            "types": "./dist/src/opcode.d.ts",
            "default": "./dist/src/opcode.js"
        },
        "./evm": {
            "types": "./dist/src/evm/index.d.ts",
            "default": "./dist/src/evm/index.js"
        },
        "./selector": {
            "types": "./dist/src/selector/index.d.ts",
            "default": "./dist/src/selector/index.js"
        }
    },
    "files": [
        "dist/src/"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/acuarica/evm.git"
    },
    "author": "Luis Mastrangelo (https://acuarica.github.io)",
    "contributors": [
        "Luit Hollander <luit@hollander.email> (https://luit.me)"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/acuarica/evm/issues"
    },
    "homepage": "https://github.com/acuarica/evm#readme",
    "keywords": [
        "ethereum",
        "evm",
        "utils",
        "decompiler",
        "typescript",
        "bytecode",
        "solidity"
    ],
    "devDependencies": {
        "@noble/hashes": "^1.3.0",
        "@swc/core": "^1.3.39",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.2",
        "@types/semver": "^7.3.12",
        "@types/yargs": "^17.0.12",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "chai": "^4.3.6",
        "chalk": "4.1.0",
        "eslint": "^8.36.0",
        "ethers": "^5.6.9",
        "http-server": "^14.1.1",
        "husky": "^8.0.1",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.4",
        "pretty-quick": "^3.1.3",
        "semver": "^7.3.7",
        "solc-0.5.17": "npm:solc@0.5.17",
        "solc-0.5.5": "npm:solc@0.5.5",
        "solc-0.6.12": "npm:solc@0.6.12",
        "solc-0.7.6": "npm:solc@0.7.6",
        "solc-0.8.16": "npm:solc@0.8.16",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "yargs": "^17.5.1"
    },
    "scripts": {
        "prepare": "husky install",
        "compile": "tsc",
        "lint": "eslint .",
        "test": "mocha",
        "test:check": "mocha --config .mocharc.check.js",
        "test:smoke": "mocha --config .mocharc.smoke.js",
        "coverage": "nyc mocha",
        "bundle": "webpack --mode production",
        "makeSelectors": "ts-node scripts/makeSelectors.ts",
        "prettier": "prettier --write .",
        "prettier:staged": "pretty-quick --staged",
        "docs": "http-server docs"
    }
}
